<div class="section">
  <h2 class="title">Results</h2>
  <table>
    <tr class="heading">
      <th>ID</th>
      <th>Date</th>
      <th>Type</th>
      <th>Value</th>
      <th>Interpretation</th>
      <th>Reference Range</th>
    </tr>
  <% result.each_with_index do |item, index| -%>
    <tr <%if index == result.length-1%>class="last"<%end%>>
      <td><%=item.id%></td>
      <td><%=item.date.value.strftime("%d %b %Y")%></td>
      <td><%=item.result_type.display_name.titleize%></td>
      <%if item.value.class == Bp -%>
      <td>Diastolic: <%=item.value.distolic%>; Systolic: <%=item.value.systolic%></td>
      <% else -%>
      <td><%=item.value.value%> <%=item.value.units%></td>
      <% end -%>
      <td><%=item.interpretation.display_name.titleize%></td>
      <td><%=item.reference_range%></td>
    </tr>
  <% end -%>
  </table>
  
  <br/><br/>
  <div>
  <% result.each_with_index do |item, index| -%>
  <%= render :partial => "shared/sections/sub_sections/abstract_section", :locals => {:description => item.description, :information_source => item.information_source} %>
  <% end -%>
  </div>
</div>